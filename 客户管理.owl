@prefix cem: <http://example.com/telecom-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

#################################################################
# 本体元数据（Ontology Metadata）
#################################################################

cem:TelecomOntology rdf:type owl:Ontology ;
    rdfs:label "电信领域本体"@zh ;
    rdfs:comment "描述电信业务核心对象、属性及关系的本体模型，支持用户分析、产品设计、服务运营及网络资源管理"@zh ;
    rdfs:comment "Holistic ontology model describing core objects, properties and relationships in telecom business, supporting user analysis, product design, service operation and network resource management"@en ;
    owl:versionInfo "1.0.0"^^xsd:string .

#################################################################
# 核心概念（Core Concepts）- 顶层类定义
#################################################################

### 1. 根类：客户体验管理（CEM顶层范畴）
cem:Customer_Experience_Management a owl:Class ;
    rdfs:label "客户体验管理"@zh ;
    rdfs:label "Customer_Experience_Management"@en ;
    rdfs:comment "围绕事件、产品、员工、服务、渠道、用户、网络等维度，整合主动预警与被动感知机制，系统优化客户体验的管理体系"@zh ;
    rdfs:comment "Holistic management system integrating proactive alerts and passive sensing mechanisms across events, products, employees, services, channels, users, and networks to optimize customer experience"@en .

### 2. 核心实体类（继承自客户体验管理）
cem:User a owl:Class ;
    rdfs:label "用户"@zh ;
    rdfs:label "User"@en ;
    rdfs:subClassOf cem:Customer_Experience_Management ;
    rdfs:comment "围绕身份信息、标签信息、基础信息及用户体验维度，是客户体验管理的核心对象"@zh ;
    rdfs:comment "Central entity representing identity, profile, basic information and experience dimensions in CEM framework"@en .

cem:Product a owl:Class ;
    rdfs:label "产品"@zh ;
    rdfs:label "Product"@en ;
    rdfs:subClassOf cem:Customer_Experience_Management ;
    rdfs:comment "包含增值服务、基础通信（固网通信、移动通信）等，满足客户通信及附加需求的业务集合"@zh ;
    rdfs:comment "Business offerings comprising value-added services and basic communications (fixed-line/mobile communications) to meet customers' telecom and additional needs"@en .

cem:Service a owl:Class ;
    rdfs:label "服务"@zh ;
    rdfs:label "Service"@en ;
    rdfs:subClassOf cem:Customer_Experience_Management ;
    rdfs:comment "涵盖装维服务、咨询热线、厅店服务、电渠服务等类型，结合服务流程保障服务质量的服务体系"@zh ;
    rdfs:comment "Service system covering installation & maintenance, consulting hotline, store service, online channel service, etc., ensuring service quality through service processes"@en .

cem:Network a owl:Class ;
    rdfs:label "网络"@zh ;
    rdfs:label "Network"@en ;
    rdfs:subClassOf cem:Customer_Experience_Management ;
    rdfs:comment "包含宽带网络、移动网络等，支撑产品/服务实现的底层网络架构资源"@zh ;
    rdfs:comment "Underlying network infrastructure resources (including broadband network, mobile network) supporting the implementation of products/services"@en .

cem:Event a owl:Class ;
    rdfs:label "事件"@zh ;
    rdfs:label "Event"@en ;
    rdfs:subClassOf cem:Customer_Experience_Management ;
    rdfs:comment "包含被动客户感知（如投诉、反馈）和主动预警（如网络故障、自然灾害、活动事件）的事件集合"@zh ;
    rdfs:comment "Event set including passive customer perception (e.g., complaints, feedback) and proactive early warning (e.g., network faults, natural disasters, events)"@en .

cem:Employee a owl:Class ;
    rdfs:label "员工"@zh ;
    rdfs:label "Employee"@en ;
    rdfs:subClassOf cem:Customer_Experience_Management ;
    rdfs:comment "为用户提供专业服务并对网络进行保障的人员"@zh ;
    rdfs:comment "Personnel providing professional services to users and ensuring network operation"@en .

cem:Channel a owl:Class ;
    rdfs:label "渠道"@zh ;
    rdfs:label "Channel"@en ;
    rdfs:subClassOf cem:Customer_Experience_Management ;
    rdfs:comment "由网络部、电渠部、市场部等部门组成，负责客户触达与业务交付的服务渠道集合"@zh ;
    rdfs:comment "Service channel set composed of network department, online channel department, marketing department, etc., responsible for customer reach and business delivery"@en .

#################################################################
# 子类定义
#################################################################

### 1. 网络相关子类
cem:BroadbandNetwork rdf:type owl:Class ;
    rdfs:subClassOf cem:Network_Resource ;  
    rdfs:label "宽带网络"@zh ;
    rdfs:label "Broadband_Network"@en ;
    rdfs:comment "宽带网络的物理/逻辑架构（如光纤链路、OLT节点、分光器布局）"@zh ;
    rdfs:comment "Physical/logical architecture of broadband network (e.g., optical fiber links, OLT nodes, splitter layout)"@en .

cem:MobileNetwork rdf:type owl:Class ;
    rdfs:subClassOf cem:Network_Resource ;  
    rdfs:label "移动网络"@zh ;
    rdfs:label "Mobile_Network"@en ;
    rdfs:comment "移动网络的基础设施布局（基站、核心网、传输网连接关系）"@zh ;
    rdfs:comment "Infrastructure layout of mobile network (connection relationships of base stations, core networks, transmission networks)"@en .

cem:Network_Resource a owl:Class ;
    rdfs:label "网络资源"@zh ;
    rdfs:label "Network_Resource"@en ;
    rdfs:subClassOf cem:Network ;
    rdfs:comment "构成网络的物理/逻辑资源集合（如链路、节点、基础设施）"@zh ;
    rdfs:comment "Collection of physical/logical resources composing the network (e.g., links, nodes, infrastructure)"@en .

### 2. 服务相关子类
cem:Installation_Maintenance_Service rdf:type owl:Class ;
    rdfs:subClassOf cem:Service ;
    rdfs:label "装维服务"@zh ;
    rdfs:label "Installation_Maintenance_Service"@en ;
    rdfs:comment "提供的上门安装、调试及故障维修服务"@zh ;
    rdfs:comment "On-site installation, debugging and fault repair services"@en .

cem:Consulting_Hotline rdf:type owl:Class ;
    rdfs:subClassOf cem:Service ;
    rdfs:label "咨询热线"@zh ;
    rdfs:label "Consulting_Hotline"@en ;
    rdfs:comment "提供业务咨询、故障申报等远程服务"@zh ;
    rdfs:comment "Remote services including business consultation and fault reporting"@en .

cem:Store_Service rdf:type owl:Class ;
    rdfs:subClassOf cem:Service ;  
    rdfs:label "厅店服务"@zh ;
    rdfs:label "Store_Service"@en ;
    rdfs:comment "实体营业厅的面对面服务（如业务办理、咨询）"@zh ;
    rdfs:comment "Face-to-face services in physical business halls (e.g., business handling, consultation)"@en .

cem:Online_Channel_Service rdf:type owl:Class ;
    rdfs:subClassOf cem:Service ;  
    rdfs:label "电渠服务"@zh ;
    rdfs:label "Online_Channel_Service"@en ;
    rdfs:comment "通过APP/微信/官网等线上平台的自助服务（如在线办理、缴费）"@zh ;
    rdfs:comment "Self-service through online platforms (APP/WeChat/Official Website) such as online handling and payment"@en .

cem:Marketing_Service rdf:type owl:Class ;
    rdfs:subClassOf cem:Service ;
    rdfs:label "营销服务"@zh ;
    rdfs:label "Marketing_Service"@en ;
    rdfs:comment "提供产品推荐、套餐优化及个性化营销方案的服务"@zh ;
    rdfs:comment "Services including product recommendation, package optimization and personalized marketing solutions"@en .

cem:Process_Step rdf:type owl:Class ;
    rdfs:subClassOf cem:Service_Process ;  
    rdfs:label "流程步骤"@zh ;
    rdfs:label "Process_Step"@en ;
    rdfs:comment "服务流程中的具体操作节点（如上门预约、现场调试、结果确认）"@zh ;
    rdfs:comment "Specific operation nodes in the service process (e.g., on-site appointment, on-site debugging, result confirmation)"@en .

cem:Service_Process a owl:Class ;
    rdfs:label "服务流程"@zh ;
    rdfs:label "Service_Process"@en ;
    rdfs:subClassOf cem:Service ;
    rdfs:comment "用户从需求提出到服务完成的端到端操作序列，包含流程步骤"@zh ;
    rdfs:comment "End-to-end operation sequence from user demand submission to service completion, including process steps"@en .

### 3. 渠道相关子类
cem:Cloud_Network_Department rdf:type owl:Class ;
    rdfs:subClassOf cem:Channel ;
    rdfs:label "云网部"@zh ;
    rdfs:label "Cloud_Network_Department"@en ;
    rdfs:comment "负责云计算、网络融合（如5G+云专网）及行业数字化解决方案的部门，支撑B端/G端业务拓展"@zh ;
    rdfs:comment "Department responsible for cloud computing, network convergence (e.g., 5G + cloud private network) and industry digital solutions, supporting B-end/G-end business expansion"@en .

cem:Marketing_Department rdf:type owl:Class ;
    rdfs:subClassOf cem:Channel ;
    rdfs:label "市场部"@zh ;
    rdfs:label "Marketing_Department"@en ;
    rdfs:comment "聚焦用户拓展、品牌推广及市场策略制定的部门，驱动业务增长与用户规模提升"@zh ;
    rdfs:comment "Department focusing on user expansion, brand promotion and marketing strategy formulation, driving business growth and user scale improvement"@en .

cem:Online_Channel_Department rdf:type owl:Class ;
    rdfs:subClassOf cem:Channel ;
    rdfs:label "电渠部"@zh ;
    rdfs:label "Online_Channel_Department"@en ;
    rdfs:comment "负责线上电子渠道（APP/微信/官网等）运营的部门，承载自助服务、用户触达及数字化营销职能"@zh ;
    rdfs:comment "Department responsible for the operation of online electronic channels (APP/Official Website/WeChat), undertaking self-service, user reach and digital marketing functions"@en .

cem:Network_Department rdf:type owl:Class ;
    rdfs:subClassOf cem:Channel ;
    rdfs:label "网络部"@zh ;
    rdfs:label "Network_Department"@en ;
    rdfs:comment "负责网络规划、建设、运维及资源管理的部门，支撑产品与服务的底层连接能力"@zh ;
    rdfs:comment "Department responsible for network planning, construction, operation and maintenance, and resource management, supporting the underlying connection capability of products and services"@en .

### 4. 事件相关子类
cem:Network_Fault rdf:type owl:Class ;
    rdfs:subClassOf cem:Passive_Sensing ;  
    rdfs:label "网络故障"@zh ;
    rdfs:label "Network_Fault"@en ;
    rdfs:comment "网络运行中出现的异常（如断网、卡顿），需通过用户反馈或设备告警感知"@zh ;
    rdfs:comment "Abnormalities in network operation (e.g., disconnection, lag), which need to be perceived through user feedback or equipment alarms"@en .

cem:User_Complaint rdf:type owl:Class ;
    rdfs:subClassOf cem:Passive_Sensing ;  
    rdfs:label "用户投诉"@zh ;
    rdfs:label "User_Complaint"@en ;
    rdfs:comment "用户对产品、服务或网络体验不满的反馈，是被动感知的核心类型"@zh ;
    rdfs:comment "User feedback on dissatisfaction with product, service or network experience, which is the core type of passive sensing"@en .

cem:Activity_Event rdf:type owl:Class ;
    rdfs:subClassOf cem:Proactive_Early_Warning ;  
    rdfs:label "活动事件"@zh ;
    rdfs:label "Activity_Event"@en ;
    rdfs:comment "涉及各类社会活动、大型集会、赛事演出等的动态信息，需提前预警网络负荷或安全风险"@zh ;
    rdfs:comment "Dynamic information related to various social activities, large gatherings, events and performances, requiring early warning of network load or safety risks"@en .

cem:Network_Warning rdf:type owl:Class ;
    rdfs:subClassOf cem:Proactive_Early_Warning ;  
    rdfs:label "网络预警"@zh ;
    rdfs:label "Network_Warning"@en ;
    rdfs:comment "基于网管告警触发的各类故障集合，或者由设备性能预估即将/可能发生的故障，提前干预以减少影响"@zh ;
    rdfs:comment "Collection of faults triggered by network management alarms, or faults predicted to occur based on equipment performance, with early intervention to reduce impact"@en .

cem:Natural_Disaster rdf:type owl:Class ;
    rdfs:subClassOf cem:Proactive_Early_Warning ;  
    rdfs:label "自然灾害"@zh ;
    rdfs:label "Natural_Disaster"@en ;
    rdfs:comment "聚焦于地震、洪水、台风、暴雨等自然现象引发的灾害风险，需提前预警以保障网络稳定"@zh ;
    rdfs:comment "Focus on disaster risks caused by natural phenomena such as earthquakes, floods, typhoons, and heavy rains, requiring early warning"@en .

cem:Passive_Sensing a owl:Class ;
    rdfs:label "被动感知"@zh ;
    rdfs:label "Passive_Sensing"@en ;
    rdfs:subClassOf cem:Event ;
    rdfs:comment "依托产品感知、服务感知、网络感知等途径，收集反馈以洞察客户体验的事件类型"@zh ;
    rdfs:comment "Event type for insight into customer experience through feedback collection via product perception, service perception, network perception"@en .

cem:Proactive_Early_Warning a owl:Class ;
    rdfs:label "主动预警"@zh ;
    rdfs:label "Proactive_Early_Warning"@en ;
    rdfs:subClassOf cem:Event ;
    rdfs:comment "通过活动事件、自然灾害、网络预警等场景，提前识别风险并干预的事件类型"@zh ;
    rdfs:comment "Event type for early risk identification and intervention through events, natural disasters, network warnings"@en .

### 5. 产品相关子类
cem:Basic_Communication_Product a owl:Class ;
    rdfs:label "基础通信产品"@zh ;
    rdfs:label "Basic_Communication_Product"@en ;
    rdfs:subClassOf cem:Product ;
    rdfs:comment "满足用户核心通信需求的产品类型（如固网通信、移动通信）"@zh ;
    rdfs:comment "Product type meeting users' core communication needs (e.g., fixed-line communication, mobile communication)"@en .

cem:Value_Added_Service a owl:Class ;
    rdfs:label "增值服务"@zh ;  
    rdfs:label "Value_Added_Service"@en ;
    rdfs:subClassOf cem:Product ;
    rdfs:comment "基础通信（语音/流量/宽带）外，为用户提供云存储、智慧家庭、企业专网等额外功能或权益，以提升体验或满足个性化需求的扩展服务"@zh ;
    rdfs:comment "Extended services beyond basic communication (voice/data/broadband), such as cloud storage, smart home, and enterprise private network, which provide additional functions or benefits to enhance user experience or meet personalized needs"@en .  

### 6. 用户相关子类
cem:User_Attribute a owl:Class ;
    rdfs:label "用户属性"@zh ;
    rdfs:label "User_Attribute"@en ;
    rdfs:subClassOf cem:User ;
    rdfs:comment "描述用户特征的属性集合（如身份、标签、基础信息）"@zh ;
    rdfs:comment "Collection of attributes describing user characteristics (e.g., identity, tag, basic information)"@en .

cem:Identity_Information a owl:Class ;
    rdfs:label "身份信息"@zh ;
    rdfs:label "Identity_Information"@en ;  
    rdfs:subClassOf cem:User ;  
    rdfs:comment "用户基础数据（各类电信产品的唯一标识集合），用于精准触达用户与产品之间的关系"@zh ;
    rdfs:comment "Basic user data (a collection of unique identifiers for various telecom products), used to accurately connect users with products"@en .  

cem:Tag_Information a owl:Class ;
    rdfs:label "标签信息"@zh ;
    rdfs:label "Tag_Information"@en ;  
    rdfs:subClassOf cem:User ;  
    rdfs:comment "用户分层标识（如“高价值用户”“潜在流失用户”）"@zh ;
    rdfs:comment "User stratification identifiers (e.g., 'high-value users', 'potential churning users')"@en .  

#################################################################
#    Object Properties
#################################################################

### 1. 员工-用户相关属性
cem:Proactive_Service rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Employee ;  
    rdfs:range cem:User ;    
    rdfs:comment "描述员工主动向用户提供服务的关系（如主动咨询、需求跟进）"@zh ;
    rdfs:label "主动服务"@zh ;
    rdfs:label "Proactive_Service"@en .

cem:User_Feedback rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:User ;
    rdfs:range cem:Employee ;
    rdfs:comment "用户根据自身感知的产品/服务/网络问题，向对应员工反馈诉求"@zh ;
    rdfs:label "用户反馈"@zh ;
    rdfs:label "User_Feedback"@en .

cem:User_Evaluate_Employee rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:User ;
    rdfs:range cem:Employee ;
    rdfs:comment "用户对员工的服务态度、专业性等进行感知评估，可通过得分量化"@zh ;
    rdfs:label "用户评价员工"@zh ;
    rdfs:label "User_Evaluate_Employee"@en .


### 2. 事件相关属性
cem:Event_Impact rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Event ;  
    rdfs:range cem:Event ;
    rdfs:comment "描述不同事件间的关联逻辑（如“网络故障”触发“用户投诉”）"@zh ;
    rdfs:label "事件影响"@zh ;
    rdfs:label "Event_Impact"@en .

cem:Event_Affect_Product rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Event ;
    rdfs:range cem:Product ;  
    rdfs:comment "描述事件对哪些核心产品产生影响（如“自然灾害”影响“宽带产品”）"@zh ;
    rdfs:label "事件影响产品"@zh ;
    rdfs:label "Event_Affect_Product"@en .

cem:Event_Optimize_Product rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Event ;
    rdfs:range cem:Product ;
    rdfs:comment "基于已发生的事件（如用户投诉、故障反馈）持续改进现有产品功能或体验"@zh ;
    rdfs:label "事件优化产品"@zh ;  
    rdfs:label "Event_Optimize_Product"@en .

cem:Network_Trigger_Event rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Network_Resource ;  
    rdfs:range cem:Event ;
    rdfs:comment "描述网络资源异常引发的具体事件（如“基站故障”引发“断网事件”）"@zh ;
    rdfs:label "网络引起事件"@zh ;
    rdfs:label "Network_Trigger_Event"@en .

cem:User_Trigger_Event rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:User ;
    rdfs:range cem:Event ;
    rdfs:comment "用户因产品/服务/网络不满意，触发被动感知事件（如投诉、反馈）"@zh ;
    rdfs:label "用户触发事件"@zh ;
    rdfs:label "User_Trigger_Event"@en .

cem:User_Track_Event rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:User ;
    rdfs:range cem:Event ;
    rdfs:comment "用户动态跟踪已触发事件的处理进度（如投诉工单状态、故障修复进度）"@zh ;
    rdfs:label "用户跟踪事件"@zh ;
    rdfs:label "User_Track_Event"@en .

cem:Event_Warn_User rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Event ;
    rdfs:range cem:User ;
    rdfs:comment "基于网络性能异常、自然灾害、大型活动等主动预警事件，提前告知用户潜在影响"@zh ;
    rdfs:label "事件预警用户"@zh ;
    rdfs:label "Event_Warn_User"@en .

cem:Employee_Handle_Event rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Employee ;
    rdfs:range cem:Event ;
    rdfs:comment "员工承接并处理各类事件（如用户投诉、网络故障工单）"@zh ;
    rdfs:label "员工处理事件"@zh ;
    rdfs:label "Employee_Handle_Event"@en .

cem:Channel_Divert_Event rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Channel ;  
    rdfs:range cem:Event ;
    rdfs:comment "不同渠道部门对跨部门事件进行转派（如“电渠部”将网络故障事件转派“网络部”）"@zh ;
    rdfs:label "渠道分流事件"@zh ;
    rdfs:label "Channel_Divert_Event"@en .


### 3. 网络相关属性
cem:Network_Resource_Interconnect rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Network_Resource ;
    rdfs:range cem:Network_Resource ;
    rdfs:comment "描述不同网络资源间的连接关系（如“OLT节点”与“分光器”互联、“基站”与“核心网”互联）"@zh ;
    rdfs:label "网络资源互联"@zh ;
    rdfs:label "Network_Resource_Interconnect"@en .

cem:Product_Depend_on_Network_Resource rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Product ;
    rdfs:range cem:Network_Resource ;
    rdfs:comment "宽带、移动等产品套餐依赖特定网络资源（如“5G套餐”依赖“5G基站”“核心网”）"@zh ;
    rdfs:label "产品依赖网络资源"@zh ;
    rdfs:label "Product_Depend_on_Network_Resource"@en .

cem:Network_Resource_Support_Product rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain cem:Network_Resource ;  
    rdfs:range cem:Product ;
