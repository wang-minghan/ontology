##################################
# 关系定义
# 必须继承ex:custom_relation
# 有正向关系就需定义一个反向关系
##################################
ex:hasUser a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:Product ;
  rdfs:range ex:user ;
  rdfs:label "使用者"@zh ;
  rdfs:comment "用途: 产品 - 使用用户. 主体: 产品(方案ID). 客体: 用户(用户ID). 来源: 产品订购/实例数据. 刷新频率: DAILY."@zh ;
  owl:inverseOf ex:ownsProduct .

ex:ownsProduct a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:user ;
  rdfs:range ex:Product ;
  rdfs:label "拥有"@zh ;
  rdfs:comment "用途: 用户 - 所拥有/已订购产品. 主体: 用户(用户ID). 客体: 产品(方案ID). 来源: 订购/权益实例表. 刷新频率: DAILY."@zh ;
  owl:inverseOf ex:hasUser .


ex:hasBehavior a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:user ;
  rdfs:range ex:Behavior ;
  rdfs:label "拥有行为"@zh ;
  rdfs:comment "用户产生的具体行为事件"@zh ;
  owl:inverseOf ex:behaviorUser .

ex:behaviorUser a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:Behavior ;
  rdfs:range ex:user ;
  rdfs:label "行为用户"@zh ;
  rdfs:comment "行为事件对应的用户"@zh ;
  owl:inverseOf ex:hasBehavior .

ex:behaviorProduct a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:Behavior ;
  rdfs:range ex:Product ;
  rdfs:label "涉及产品"@zh ;
  rdfs:comment "行为关联到的产品(办理/推荐/触达对象)"@zh ;
  owl:inverseOf ex:productBehavior .

ex:productBehavior a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:Product ;
  rdfs:range ex:Behavior ;
  rdfs:label "被行为涉及"@zh ;
  rdfs:comment "某产品被哪些行为事件引用"@zh ;
  owl:inverseOf ex:behaviorProduct .

ex:hasTag a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:user ;
  rdfs:range ex:tag ;
  rdfs:label "拥有标签"@zh ;
  rdfs:comment "用户当前具备的离散标签(画像/状态)"@zh ;
  owl:inverseOf ex:tagOfUser .

ex:tagOfUser a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:tag ;
  rdfs:range ex:user ;
  rdfs:label "标签所属用户"@zh ;
  rdfs:comment "该标签被哪些用户拥有"@zh ;
  owl:inverseOf ex:hasTag .

ex:hasMetric a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:user ;
  rdfs:range ex:metric ;
  rdfs:label "拥有指标"@zh ;
  rdfs:comment "用户粒度指标(宽表 u_* 列映射)"@zh ;
  owl:inverseOf ex:metricOfUser .

ex:metricOfUser a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:metric ;
  rdfs:range ex:user ;
  rdfs:label "指标所属用户"@zh ;
  rdfs:comment "该指标的用户主体"@zh ;
  owl:inverseOf ex:hasMetric .

ex:productMetric a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:Product ;
  rdfs:range ex:metric ;
  rdfs:label "产品指标"@zh ;
  rdfs:comment "产品粒度指标(宽表 p_* 列映射)"@zh ;
  owl:inverseOf ex:metricOfProduct .

ex:metricOfProduct a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:metric ;
  rdfs:range ex:Product ;
  rdfs:label "指标所属产品"@zh ;
  rdfs:comment "该指标归属的产品对象"@zh ;
  owl:inverseOf ex:productMetric .

ex:tagDerivedFromMetric a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:tag ;
  rdfs:range ex:metric ;
  rdfs:label "标签来源指标"@zh ;
  rdfs:comment "标签由某指标/模型结果阈值派生 (如 套餐升级意向 - 套餐升级概率)"@zh ;
  owl:inverseOf ex:metricDerivesTag .

ex:metricDerivesTag a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:metric ;
  rdfs:range ex:tag ;
  rdfs:label "指标派生标签"@zh ;
  rdfs:comment "指标或模型输出派生的离散标签集合"@zh ;
  owl:inverseOf ex:tagDerivedFromMetric .


ex:tagMapsProduct a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:tag ;
  rdfs:range ex:Product ;
  rdfs:label "标签映射产品"@zh ;
  rdfs:comment "标签值（如 学生用户/老年用户）来源于主套餐对应的产品属性"@zh ;
  owl:inverseOf ex:productMappedByTag .

ex:productMappedByTag a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:Product ;
  rdfs:range ex:tag ;
  rdfs:label "产品派生标签"@zh ;
  rdfs:comment "该产品特征被用来派生哪些用户标签"@zh ;
  owl:inverseOf ex:tagMapsProduct .
