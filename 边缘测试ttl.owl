# 边缘测试：unionOf 写法关系（用于验证解析兼容性）
# A=NetworkOptimizationService -> B=InstallationMaintenanceService
ex:edgeLinkAB a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain ex:NetworkOptimizationService ;
  rdfs:range [ a owl:Class ; owl:unionOf ( ex:InstallationMaintenanceService ex:HotlineService ) ] ;
  rdfs:label "边缘测试AB-段1"@zh ;
  rdfs:comment "边缘测试：同名ObjectProperty(edgeLinkAB) 段1；range使用[a owl:Class ; owl:unionOf (...)]，包含2个对象"@zh ;
  ex:domainAttribute ex:服务唯一标识 ;
  ex:rangeAttribute ex:服务唯一标识 .

ex:edgeLinkBA a owl:ObjectProperty ;
  rdfs:subPropertyOf ex:custom_relation ;
  rdfs:domain [ a owl:Class ; owl:unionOf ( ex:InstallationMaintenanceService ex:HotlineService ) ] ;
  rdfs:range ex:NetworkOptimizationService ;
  rdfs:label "边缘测试BA-段1反向"@zh ;
  rdfs:comment "边缘测试：edgeLinkAB 的反向关系（段1）"@zh ;
  ex:domainAttribute ex:服务唯一标识 ;
  ex:rangeAttribute ex:服务唯一标识 .

# B=InstallationMaintenanceService -> C=HotlineService
