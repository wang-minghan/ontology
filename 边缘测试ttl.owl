@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://asiainfo.com/example-owl#> .

##################################
# 基础抽象类与通用语义（禁止变动）
##################################
ex:Entity a owl:Class ; rdfs:subClassOf prov:Entity ; rdfs:label "实体"@zh .
ex:Object a owl:Class ; rdfs:subClassOf ex:Entity ; rdfs:label "本体对象"@zh .
ex:Action a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "行动"@zh .
ex:Logic a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "决策/逻辑函数"@zh .
ex:hasPrimaryKey a owl:AnnotationProperty ; rdfs:label "主键属性"@zh ; rdfs:comment "在类级别声明该对象的主键数据属性"@zh .
ex:hasTitle a owl:AnnotationProperty ; rdfs:label "标题属性"@zh ; rdfs:comment "在类级别声明该对象的标题数据属性"@zh .
ex:bindAction a owl:ObjectProperty ; rdfs:label "本体对象关联行动"@zh ; rdfs:domain ex:Object ; rdfs:range ex:Action .
ex:bindLogic a owl:ObjectProperty ; rdfs:label "本体对象关联函数"@zh ; rdfs:domain ex:Object ; rdfs:range ex:Logic .
ex:custom_relation a owl:ObjectProperty ; rdfs:label "自定义对象与对象之间的关系"@zh .
ex:Parameter a owl:Class ; rdfs:label "函数参数"@zh .
ex:hasParameter a owl:ObjectProperty ; rdfs:label "有参数"@zh ; rdfs:domain ex:Logic ; rdfs:range ex:Parameter .
ex:paramName a owl:DatatypeProperty ; rdfs:label "参数名"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:string .
ex:paramType a owl:DatatypeProperty ; rdfs:label "参数类型"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:string .
ex:paramTypeNode a owl:ObjectProperty ; rdfs:label "参数类型节点"@zh ; rdfs:domain ex:Parameter .
ex:ListType a owl:Class ; rdfs:label "列表类型"@zh .
ex:DictType a owl:Class ; rdfs:label "字典类型"@zh .
ex:itemType a owl:DatatypeProperty ; rdfs:label "元素类型"@zh .
ex:keyType a owl:DatatypeProperty ; rdfs:label "键类型"@zh .
ex:valueType a owl:DatatypeProperty ; rdfs:label "值类型"@zh .
ex:isOptional a owl:DatatypeProperty ; rdfs:label "是否可选"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:boolean .
ex:defaultValue a owl:DatatypeProperty ; rdfs:label "默认值"@zh ; rdfs:domain ex:Parameter ; rdfs:range xsd:string .
ex:ontologyName a owl:Class ; rdfs:label "本体名称"@zh .
ex:domainAttribute a owl:AnnotationProperty ; rdfs:label "域属性"@zh ; rdfs:comment "指定对象属性的域端使用哪个数据属性建立关联"@zh .
ex:rangeAttribute a owl:AnnotationProperty ; rdfs:label "值域属性"@zh ; rdfs:comment "指定对象属性的值域端使用哪个数据属性建立关联"@zh .
ex:hasMiddleObject a owl:ObjectProperty ; rdfs:label "中间对象"@zh ; rdfs:comment "用来描述多对多关系的情况下，两个对象所使用到的中间对象"@zh .
##################################
# 本体名称
##################################
ex:CSAT_customer_satisfaction a ex:ontologyName ;
  rdfs:label "客户服务体验(满意度分析)"@zh .

##################################
# 可用函数
##################################
ex:get_prediction a ex:Logic ;
  rdfs:label "调用预测模型获取满意度预测结果"@zh ;
  rdfs:comment """调用满意度预测模型，获取满意度得分预测以及TOP3预测指标（技术指标）和技术指标所对应的业务指标。
    业务指标是根据业务经验映射得到的可量化指标，用于反映产品体验状态。预测模型返回的业务指标对应的客户产品使用满意度中相关的属性。
    需要根据属性的具体值，结合业务指标，来判断哪些issue_type是导致客户满意度低的潜在原因。支持批量预测。
    必须通过该函数来获取满意度得分信息。
    入参：
    prod_inst_ids -> 列表，包含需要进行预测的产品实例id"""@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "prod_inst_ids" ; ex:paramType "list" ; ex:isOptional false ; ex:paramTypeNode [ a ex:ListType ] ] .

ex:get_solution a ex:Logic ;
  rdfs:label "根据问题类型获取修复策略"@zh ;
  rdfs:comment "当判断出导致客户满意度低的issue_type为哪些后，调用该函数来为这些issue_type获取相应的修复策略"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "issue_types" ; ex:paramType "list" ; ex:isOptional false ; ex:paramTypeNode [ a ex:ListType ] ] .

ex:is_low_satisfaction a ex:Logic ;
  rdfs:label "判断是否属于低满意度"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "scores" ; ex:paramType "list" ; ex:isOptional false ; ex:paramTypeNode [ a ex:ListType ] ] .

ex:simulate_satisfaction_improvement a ex:Logic ;
  rdfs:label "模拟仿真可提升的满意度数值"@zh ;
  rdfs:comment """Mock simulation logic to estimate satisfaction improvement for a given strategy.
    Returns a random value in [1.0, 3.0] with one decimal place."""@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "strategy_text" ; ex:paramType "xsd:string" ; ex:isOptional false ] .

##################################
# Action动作定义
##################################
ex:assign_order_to_employee a ex:Action ;
  rdfs:label "分配工单"@zh ;
  rdfs:comment """@action_label: 分配工单
    分配工单给指定员工"""@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "workorder_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "employee_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] .

ex:work_order ex:bindAction ex:assign_order_to_employee .

ex:create_hotlines_order a ex:Action ;
  rdfs:label "创建万号服务工单"@zh ;
  rdfs:comment "@action_label: 创建万号服务工单"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "cust_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "service_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "prod_inst_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "complaint_type" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "complaint_details" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "plan_id" ; ex:paramType "xsd:string" ; ex:isOptional true ] .

ex:HotlineService ex:bindAction ex:create_hotlines_order .

ex:create_installation_order a ex:Action ;
  rdfs:label "创建装维服务工单"@zh ;
  rdfs:comment "@action_label: 创建装维服务工单"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "service_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "cust_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "prod_inst_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "issue_description" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "plan_id" ; ex:paramType "xsd:string" ; ex:isOptional true ] .

ex:InstallationMaintenanceService ex:bindAction ex:create_installation_order .

ex:create_network_optimize_order a ex:Action ;
  rdfs:label "创建网优服务工单"@zh ;
  rdfs:comment """@action_label: 创建网优服务工单
    根据网络优化需求创建工单，主要处理信号覆盖、网络质量等问题"""@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "service_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "cust_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "prod_inst_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "issue_description" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "plan_id" ; ex:paramType "xsd:string" ; ex:isOptional true ] .

ex:NetworkOptimizationService ex:bindAction ex:create_network_optimize_order .

ex:customer_portrait a ex:Action ;
  rdfs:label "客户画像"@zh ;
  rdfs:comment "@action_label: 客户画像"@zh  .

ex:csat_customer ex:bindAction ex:customer_portrait .

ex:edit_work_order a ex:Action ;
  rdfs:label "编辑工单"@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "关联的建议方案ID" ; ex:paramType "xsd:int" ; ex:isOptional true ] ,
                    [ a ex:Parameter ; ex:paramName "客户反馈信息" ; ex:paramType "xsd:string" ; ex:isOptional true ] ,
                    [ a ex:Parameter ; ex:paramName "客户ID" ; ex:paramType "xsd:string" ; ex:isOptional true ] ,
                    [ a ex:Parameter ; ex:paramName "优先级" ; ex:paramType "xsd:string" ; ex:isOptional true ] ,
                    [ a ex:Parameter ; ex:paramName "创建工单的服务ID" ; ex:paramType "xsd:string" ; ex:isOptional true ] ,
                    [ a ex:Parameter ; ex:paramName "产品实例ID" ; ex:paramType "xsd:string" ; ex:isOptional true ] ,
                    [ a ex:Parameter ; ex:paramName "工单状态" ; ex:paramType "xsd:string" ; ex:isOptional true ] .

ex:work_order ex:bindAction ex:edit_work_order .

ex:insert_new_plan a ex:Action ;
  rdfs:label "新增建议方案"@zh ;
  rdfs:comment """@action_label: 新增建议方案
    将生成的建议方案进行保存"""@zh ;
  ex:hasParameter [ a ex:Parameter ; ex:paramName "content" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "cust_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "prod_inst_id" ; ex:paramType "xsd:string" ; ex:isOptional false ] ,
                    [ a ex:Parameter ; ex:paramName "used_stra_ids" ; ex:paramType "xsd:string" ; ex:isOptional false ] .

ex:Plan ex:bindAction ex:insert_new_plan .

##################################
# 逻辑函数关联定义 (bindLogic)
##################################
ex:Customer_Satisfaction ex:bindLogic ex:is_low_satisfaction .
ex:Customer_Satisfaction ex:bindLogic ex:simulate_satisfaction_improvement .
ex:Repair_Strategy ex:bindLogic ex:simulate_satisfaction_improvement .
ex:Repair_Strategy ex:bindLogic ex:get_solution .
ex:Product ex:bindLogic ex:get_prediction .

