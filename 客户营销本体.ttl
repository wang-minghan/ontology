@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://asiainfo.com/example-owl#> .

##################################
# 基础抽象类与通用语义（禁止变动）
##################################
ex:Entity a owl:Class ; rdfs:subClassOf prov:Entity ; rdfs:label "实体"@zh .
ex:Object a owl:Class ; rdfs:subClassOf ex:Entity ; rdfs:label "本体对象"@zh .
ex:Action a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "行动"@zh .
ex:Function a owl:Class ; rdfs:subClassOf prov:Activity ; rdfs:label "决策/逻辑函数"@zh .
ex:isPrimaryKey a owl:AnnotationProperty ; rdfs:label "是否主键"@zh .
ex:bindAction a owl:ObjectProperty ; rdfs:label "本体对象关联行动"@zh ; rdfs:domain ex:Object ; rdfs:range ex:Action .
ex:bindFunction a owl:ObjectProperty ; rdfs:label "本体对象关联函数"@zh ; rdfs:domain ex:Object ; rdfs:range ex:Function .
ex:custom_relation a owl:ObjectProperty ; rdfs:label "自定义对象与对象之间的关系"@zh .

##################################
# 对象定义(必须继承ex:Object)
##################################
ex:asset a owl:Class ;
  rdfs:subClassOf ex:Object ;
  rdfs:label "资产"@zh ;
  rdfs:comment "用户订购销售品后，实例化出来对应的资产，已经当前资产开始服务的时间，结束服务的时间"@zh .

ex:Customer a owl:Class ;
  rdfs:subClassOf ex:Object ;
  rdfs:label "客户"@zh ;
  rdfs:comment "客户"@zh .

ex:Customer_portrait a owl:Class ;
  rdfs:subClassOf ex:Object ;
  rdfs:label "客户画像"@zh ;
  rdfs:comment ""@zh .

ex:Items_for_sale a owl:Class ;
  rdfs:subClassOf ex:Object ;
  rdfs:label "销售品"@zh ;
  rdfs:comment "销售品是用户订购的基本单位，用户订购销售品，完成销售行为"@zh .

ex:products a owl:Class ;
  rdfs:subClassOf ex:Object ;
  rdfs:label "主产品"@zh ;
  rdfs:comment """产品分为主产品和子产品
一个子产品拥有多个销售品，一个销售品归属多个子产品"""@zh .

ex:sub_product a owl:Class ;
  rdfs:subClassOf ex:Object ;
  rdfs:label "子产品"@zh ;
  rdfs:comment "子产品"@zh .

